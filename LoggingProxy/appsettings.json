{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning",
      "Yarp.ReverseProxy.Forwarder.HttpForwarder": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "mender": {
        "ClusterId": "mender",
        "Match": {
          "Path": "{**catch-all}",
          "Methods": [
            "GET",
            "PUT",
            "PATCH",
            "DELETE",
            "OPTIONS",
            "POST"
          ]
        }
      }
    },
    "Clusters": {
      "mender": {
        "Destinations": {
          "mender": {
            "Address": "https://mender.acaad.dev/"
          }
        }
      }
    }
  }
}
